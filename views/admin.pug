doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Ayush Nandanwar 
        style 
            include ../static/style.css
            include ../static/mobile.css

    body 
        nav#navbar 
            img#logo(src="https://w7.pngwing.com/pngs/738/965/png-transparent-web-development-web-design-software-development-web-developer-web-design-blue-angle-web-design-thumbnail.png", alt="Logo") 
            ul#navItems 
                li #[a(href="#introSection") Home]
                li #[a(href="#aboutSection") About]
                li #[a(href="#mySkillsSection") My Skills]
                li #[a(href="#contactSection") Contact Me]
            a.btnClose#toggle(onclick="fun()") Show

        section#myUsers 
            h1.heading Admin Dashboard
            each val,x in contact
                div.user
                    div Name:
                        div.name= contact[x].name
                    div Email:
                        div.email= contact[x].email
                    div Mobile:
                        div.mobile= contact[x].phone
                    div  Message
                        div.message= contact[x].message
        
                
        footer#footer 
            | Copyright &copy; 2021-22 | Ayush Nandnawar | All rights reserved
        script(src="../static/index.js") 
        
